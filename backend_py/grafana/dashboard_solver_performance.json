{
    "annotations": {
        "list": [
            {
                "builtIn": 1,
                "datasource": "-- Grafana --",
                "enable": true,
                "hide": true,
                "iconColor": "rgba(0, 211, 255, 1)",
                "name": "Annotations & Alerts",
                "type": "dashboard"
            }
        ]
    },
    "editable": true,
    "gnetId": null,
    "graphTooltip": 0,
    "id": null,
    "links": [],
    "panels": [
        {
            "title": "Phase Duration P95 (5m)",
            "type": "timeseries",
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 0,
                "y": 0
            },
            "targets": [
                {
                    "expr": "histogram_quantile(0.95, sum by (le) (rate(solver_phase_duration_seconds_bucket{phase=\"phase1\"}[5m])))",
                    "legendFormat": "Phase 1 (Select)",
                    "refId": "A"
                },
                {
                    "expr": "histogram_quantile(0.95, sum by (le) (rate(solver_phase_duration_seconds_bucket{phase=\"phase2\"}[5m])))",
                    "legendFormat": "Phase 2 (Assign)",
                    "refId": "B"
                },
                {
                    "expr": "histogram_quantile(0.95, sum by (le) (rate(solver_phase_duration_seconds_bucket{phase=\"lns\"}[5m])))",
                    "legendFormat": "LNS (Refine)",
                    "refId": "C"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "s"
                }
            }
        },
        {
            "title": "PT Ratio (Median)",
            "type": "timeseries",
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 12,
                "y": 0
            },
            "targets": [
                {
                    "expr": "histogram_quantile(0.5, sum by (le) (rate(solver_pt_ratio_bucket[30m])))",
                    "legendFormat": "Median PT Ratio",
                    "refId": "A"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "percentunit",
                    "thresholds": {
                        "mode": "absolute",
                        "steps": [
                            {
                                "color": "green",
                                "value": null
                            },
                            {
                                "color": "orange",
                                "value": 0.2
                            },
                            {
                                "color": "red",
                                "value": 0.25
                            }
                        ]
                    }
                }
            }
        },
        {
            "title": "Path Selection Distribution",
            "type": "piechart",
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 0,
                "y": 8
            },
            "targets": [
                {
                    "expr": "sum by (path) (increase(solver_path_selection_total[1h]))",
                    "legendFormat": "{{path}}",
                    "refId": "A"
                }
            ]
        },
        {
            "title": "Median Driver Counts (Total)",
            "type": "timeseries",
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 12,
                "y": 8
            },
            "targets": [
                {
                    "expr": "histogram_quantile(0.5, sum by (le) (rate(solver_driver_count_bucket{driver_type=\"total\"}[30m])))",
                    "legendFormat": "Drivers (Total)",
                    "refId": "A"
                },
                {
                    "expr": "histogram_quantile(0.5, sum by (le) (rate(solver_driver_count_bucket{driver_type=\"fte\"}[30m])))",
                    "legendFormat": "Drivers (FTE)",
                    "refId": "B"
                }
            ]
        }
    ],
    "refresh": "1m",
    "schemaVersion": 30,
    "style": "dark",
    "tags": [
        "shift-optimizer",
        "performance"
    ],
    "templating": {
        "list": [
            {
                "name": "job",
                "type": "query",
                "datasource": "Prometheus",
                "query": "label_values(solver_run_total, job)",
                "includeAll": true,
                "multi": true
            },
            {
                "name": "instance",
                "type": "query",
                "datasource": "Prometheus",
                "query": "label_values(solver_run_total, instance)",
                "includeAll": true,
                "multi": true
            }
        ]
    },
    "time": {
        "from": "now-6h",
        "to": "now"
    },
    "timepicker": {},
    "timezone": "",
    "title": "Shift Optimizer - Solver Performance",
    "uid": "shift-opt-perf",
    "version": 1
}