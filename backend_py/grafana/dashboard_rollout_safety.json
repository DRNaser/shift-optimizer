{
    "annotations": {
        "list": [
            {
                "builtIn": 1,
                "datasource": "-- Grafana --",
                "enable": true,
                "hide": true,
                "iconColor": "rgba(0, 211, 255, 1)",
                "name": "Annotations & Alerts",
                "type": "dashboard"
            }
        ]
    },
    "editable": true,
    "gnetId": null,
    "graphTooltip": 0,
    "id": null,
    "links": [],
    "panels": [
        {
            "title": "Budget Overrun Rate (Critical)",
            "type": "timeseries",
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 0,
                "y": 0
            },
            "targets": [
                {
                    "expr": "increase(solver_budget_overrun_total{phase=\"total\"}[30m]) / clamp_min(increase(solver_signature_runs_total[30m]), 1)",
                    "legendFormat": "Overrun Rate (30m)",
                    "refId": "A"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "color": {
                        "mode": "thresholds"
                    },
                    "thresholds": {
                        "mode": "absolute",
                        "steps": [
                            {
                                "color": "green",
                                "value": null
                            },
                            {
                                "color": "red",
                                "value": 0.02
                            }
                        ]
                    },
                    "unit": "percentunit"
                }
            }
        },
        {
            "title": "Signature Uniqueness (LRU)",
            "type": "timeseries",
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 12,
                "y": 0
            },
            "targets": [
                {
                    "expr": "increase(solver_signature_unique_total[1h]) / clamp_min(increase(solver_signature_runs_total[1h]), 1)",
                    "legendFormat": "Uniqueness Ratio (1h)",
                    "refId": "A"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "min": 0,
                    "max": 1.2,
                    "unit": "percentunit",
                    "thresholds": {
                        "mode": "absolute",
                        "steps": [
                            {
                                "color": "red",
                                "value": null
                            },
                            {
                                "color": "green",
                                "value": 0.95
                            }
                        ]
                    }
                }
            }
        },
        {
            "title": "2-Tour Starvation Monitoring",
            "type": "timeseries",
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 0,
                "y": 8
            },
            "targets": [
                {
                    "expr": "sum(increase(solver_candidates_raw_total{size=\"2er\"}[30m]))",
                    "legendFormat": "Raw Candidates (2er)",
                    "refId": "A"
                },
                {
                    "expr": "sum(increase(solver_candidates_kept_total{size=\"2er\"}[30m]))",
                    "legendFormat": "Kept Candidates (2er)",
                    "refId": "B"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "custom": {
                        "fillOpacity": 10,
                        "showPoints": "auto"
                    }
                }
            }
        },
        {
            "title": "Solver Infeasibility Count",
            "type": "stat",
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 12,
                "y": 8
            },
            "targets": [
                {
                    "expr": "increase(solver_infeasible_total[1h])",
                    "legendFormat": "Infeasible Runs (1h)",
                    "refId": "A"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "color": {
                        "mode": "thresholds"
                    },
                    "thresholds": {
                        "mode": "absolute",
                        "steps": [
                            {
                                "color": "green",
                                "value": null
                            },
                            {
                                "color": "red",
                                "value": 1
                            }
                        ]
                    }
                }
            }
        }
    ],
    "refresh": "1m",
    "schemaVersion": 30,
    "style": "dark",
    "tags": [
        "shift-optimizer",
        "canary"
    ],
    "templating": {
        "list": [
            {
                "name": "job",
                "type": "query",
                "datasource": "Prometheus",
                "query": "label_values(solver_run_total, job)",
                "includeAll": true,
                "multi": true
            },
            {
                "name": "instance",
                "type": "query",
                "datasource": "Prometheus",
                "query": "label_values(solver_run_total, instance)",
                "includeAll": true,
                "multi": true
            }
        ]
    },
    "time": {
        "from": "now-6h",
        "to": "now"
    },
    "timepicker": {},
    "timezone": "",
    "title": "Shift Optimizer - Rollout Safety",
    "uid": "shift-opt-safety",
    "version": 1
}